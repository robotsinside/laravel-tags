{
    "name": "robotsinside/laravel-tags",
    "license": "MIT",
    "description": "A package for tagging Laravel Eloquent models.",
    "authors": [
        {
            "name": "Rob Francken",
            "email": "robertfrancken@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RobotsInside\\Tags\\": "src"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^6.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RobotsInside\\Tags\\TagsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/LessonStub.php",
            "tests/stubs/TagStub.php"
        ]
    }
}